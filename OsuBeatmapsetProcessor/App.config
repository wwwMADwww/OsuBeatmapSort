<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
  </configSections>

  <!-- https://github.com/nlog/NLog/wiki/Configuration-file -->
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target name="loggerFileTarget" xsi:type="File" layout="${longdate}|${level}|${message}" fileName="OsuBeatmapsetProcessor.txt"/>
      <target name="loggerConsoleTarget" xsi:type="Console" layout="${time}|${level}|${message}"/>
    </targets>
    <rules>
      <logger name="logger" minlevel="Info" writeTo="loggerFileTarget, loggerConsoleTarget" />
    </rules>
  </nlog>
  
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
